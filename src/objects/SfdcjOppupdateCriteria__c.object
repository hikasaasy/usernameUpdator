<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <deploymentStatus>Deployed</deploymentStatus>
    <description>商談一括更新の検索画面の生成に利用します。
データの入力は行わず、項目も変更しないようにお願い致します。</description>
    <fields>
        <fullName>OpportunitiesOwner__c</fullName>
        <label>所有者</label>
        <picklist>
            <picklistValues>
                <fullName>すべての商談</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>endDate__c</fullName>
        <description>検索条件作成用の為、変更しないで下さい。</description>
        <label>終了日</label>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>name__c</fullName>
        <label>商談名</label>
        <length>255</length>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>sortOrder__c</fullName>
        <label>順序</label>
