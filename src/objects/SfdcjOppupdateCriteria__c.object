            <picklistValues>
                <fullName>降順</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>昇順</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>sort__c</fullName>
        <label>ソート</label>
        <picklist>
            <picklistValues>
                <fullName>最終更新日</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>startDate__c</fullName>
        <description>検索条件作成用の為、変更しないで下さい。</description>
        <label>開始日</label>
        <type>Date</type>
    </fields>
    <label>商談検索用クライテリア</label>
    <nameField>
        <displayFormat>{0000}</displayFormat>
        <label>Name</label>
        <type>AutoNumber</type>
    </nameField>
    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
</CustomObject>
