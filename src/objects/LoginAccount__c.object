    <fields>
        <fullName>StatusSignal__c</fullName>
        <formula>IF(Valid__c,
  IF(ISNULL(TrialExpirationDate__c),
    IMAGE(&apos;/resource/1311880464000/status_signal_zip/green.png&apos;, &apos;green&apos;, 24, 24),
    IF(TrialExpirationDate__c - NOW() &lt; 0,
      IMAGE(&apos;/resource/1311880464000/status_signal_zip/gray.png&apos;, &apos;gray&apos;, 24, 24),
      IF(TrialExpirationDate__c - NOW() &lt; 31,
        IMAGE(&apos;/resource/1311880464000/status_signal_zip/red.png&apos;, &apos;red&apos;, 24, 24),
        IF(TrialExpirationDate__c - NOW() &lt; 366/2,
          IMAGE(&apos;/resource/1311880464000/status_signal_zip/yellow.png&apos;, &apos;yellow&apos;, 24, 24),
          IMAGE(&apos;/resource/1311880464000/status_signal_zip/green.png&apos;, &apos;green&apos;, 24, 24)
        )
      )
    )
  ),
  IMAGE(&apos;/resource/1311880464000/status_signal_zip/gray.png&apos;, &apos;gray&apos;, 24, 24)
)</formula>
